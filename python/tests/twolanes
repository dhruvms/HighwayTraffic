stuff "kill -9 $(pgrep -f 'port 940')^M"
stuff "kill -9 $(pgrep -f 'port 941')^M"

stuff "python train_baseline.py --env-name LaneFollow-v1 --algo ppo --use-gae --lr 2.5e-4 --clip-param 0.1 --value-loss-coef 0.5 --num-processes 20 --num-steps 128 --num-mini-batch 4 --log-interval 1 --use-linear-lr-decay --entropy-coef 0.0 --cars 42 --base-port 9400 --length 1000.0 --lanes 2 --change --beta-dist --occupancy --gamma 0.9^M"
