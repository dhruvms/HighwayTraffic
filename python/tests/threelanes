source screenrc.common

screen -t proc1
stuff "cd ../algs^M"
stuff "p35ws^M"

stuff "python train_baseline.py --env-name LaneFollow-v1 --algo ppo --use-gae --lr 2.5e-4 --clip-param 0.1 --value-loss-coef 0.5 --num-processes 20 --num-steps 128 --ppo-epoch 16 --log-interval 1 --use-linear-lr-decay --entropy-coef 0.01 --cars 50 --base-port 9500 --length 1000.0 --lanes 3 --both --clamp-in-sim --occupancy --gamma 0.9^M"
stuff "kill -9 $(pgrep -f 'port 950')^M"
stuff "kill -9 $(pgrep -f 'port 951')^M"

stuff "python train_baseline.py --env-name LaneFollow-v1 --algo ppo --use-gae --lr 2.5e-4 --clip-param 0.1 --value-loss-coef 0.5 --num-processes 20 --num-steps 128 --num-mini-batch 4 --log-interval 1 --use-linear-lr-decay --entropy-coef 0.01 --cars 51 --base-port 9500 --length 1000.0 --lanes 3 --both --clamp-in-sim --occupancy --gamma 0.9^M"
stuff "kill -9 $(pgrep -f 'port 950')^M"
stuff "kill -9 $(pgrep -f 'port 951')^M"

stuff "python train_baseline.py --env-name LaneFollow-v1 --algo ppo --use-gae --lr 2.5e-4 --clip-param 0.1 --value-loss-coef 0.5 --num-processes 20 --num-steps 128 --num-mini-batch 4 --log-interval 1 --use-linear-lr-decay --entropy-coef 0.01 --cars 52 --base-port 9500 --length 1000.0 --lanes 3 --both --clamp-in-sim --occupancy --gamma 0.9 --fov 20^M"
stuff "kill -9 $(pgrep -f 'port 950')^M"
stuff "kill -9 $(pgrep -f 'port 951')^M"

screen -t bash
stuff "p35ws^M"
